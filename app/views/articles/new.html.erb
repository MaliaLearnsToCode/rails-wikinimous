<div class="row">
  <div class="col-8 mx-auto">
    <h1>New article</h1>
      <%= form_for(@article) do |f| %>
      <%= f.label :title %>
        <%= f.text_field :title %>
        <%= f.label :content %>
        <%= f.text_field :content %>
        <%= f.submit %>
      <% end %>
  </div>
</div>
